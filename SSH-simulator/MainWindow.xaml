<Window x:Class="SSH_simulator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SSH_simulator"
        mc:Ignorable="d"
        Title="MainWindow" Height="1099.957" Width="1685.24">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="359*" />
            <ColumnDefinition Width="430*" />
        </Grid.ColumnDefinitions>
        <TextBox x:Name="textBox_client" HorizontalAlignment="Left" Height="300" Margin="10,423,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="700" />
        <TextBox x:Name="textBox_server" Grid.Column="1" HorizontalAlignment="Left" Height="300" Margin="10,423,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="700" />
        <TextBox x:Name="textBox_client_decoded" HorizontalAlignment="Left" Height="300" Margin="10,760,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="700" />
        <TextBox x:Name="textBox_server_decoded" Grid.Column="1" HorizontalAlignment="Left" Height="300" Margin="10,760,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="700" />
        <TabControl x:Name="tabControl_client" HorizontalAlignment="Left" Height="324" Margin="10,10,0,0" VerticalAlignment="Top" Width="733">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5" />
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5" />
            </TabItem>
        </TabControl>
        <TabControl x:Name="tabControl_server" Grid.Column="1" HorizontalAlignment="Left" Height="324" Margin="10,10,0,0" VerticalAlignment="Top" Width="733">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5" />
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>